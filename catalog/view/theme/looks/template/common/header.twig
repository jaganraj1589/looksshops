<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

<title>{{ title }}</title>
<base href="{{ base }}" />
{% if description %}
<meta name="description" content="{{ description }}" />
{% endif %}
{% if keywords %}
<meta name="keywords" content="{{ keywords }}" />
{% endif %}
<script src="catalog/view/javascript/jquery/jquery-2.1.1.min.js" type="text/javascript"></script>
<!-- <link href="catalog/view/javascript/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen" /> -->
<script src="catalog/view/javascript/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<link rel="stylesheet" href="catalog/view/theme/looks/stylesheet/bootstrap.css">
 <link rel="stylesheet" href="catalog/view/theme/looks/stylesheet/style2.css"> 
<link rel="stylesheet" href="catalog/view/theme/looks/stylesheet/style.css">
<link rel="stylesheet" href="catalog/view/theme/looks/stylesheet/animate.css">

<link rel="stylesheet" href="catalog/view/theme/looks/font-awesome-4.6.3/css/font-awesome.css" type="text/css">
{# <link href="//fonts.googleapis.com/css?family=Open+Sans:400,400i,300,700" rel="stylesheet" type="text/css" /> #}
 <!-- <link href="catalog/view/theme/looks/stylesheet/stylesheet.css" rel="stylesheet"> -->
{% for style in styles %}
<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
{% endfor %}
{% for script in scripts %}
<script src="{{ script }}" type="text/javascript"></script>
{% endfor %}
<script src="catalog/view/javascript/common.js" type="text/javascript"></script>
{% for link in links %}
<link href="{{ link.href }}" rel="{{ link.rel }}" />
{% endfor %}
{% for analytic in analytics %}
{{ analytic }}
{% endfor %}
</head>
<body>

<div class="header-C">


<div class="top-bar">

  <div class="container">

    <div class="row d-flex align-items-center">

      <div class="col-md-6 d-md-block ">

        <ul class="social-custom list-inline">

          <li class="list-inline-item"><a href="{{contact}}"><i class="fa fa-phone"></i>{{telephone}}</a></li>

          <li class="list-inline-item"><a href="mailto:{{email}}" target="__blank"><i class="fa fa-envelope"></i>{{email}}</a></li>

        </ul>

      </div>

      <div class="col-md-6">

        <div class="d-flex justify-content-md-end justify-content-between">

          <div class="login"> <a href="{{returns}}" class="login-btn"><i class="fa fa-truck"></i><span class=" d-md-inline-block">Free Delivery & Returns</span></a> <a href="{{account}}" class="signup-btn" ><i class="fa fa-user"></i><span class=" d-md-inline-block">Register or Sign in</span></a> </div>

        </div>

      </div>

    </div>

  </div>

</div>

<header id="header">

  <div class="container">

    <div class="row">

      <div class="col-lg-4  col-3 lgo lgc ">

        <div class="logoC"><a href="{{home}}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}"></a></div>

        <h6>Clean and Green is<br>

          our Perfect Dream.</h6>

      </div>

     {{ search }}

      <div class="col-lg-3 col-9" >

        <div class="flt-right">

          <div class="cart-icon">

            <div class="cart-buck"><a href="{{ shopping_cart }}"><img src="img/cart.png"></a></div>

            <div class="cart-count">{{ cart_count }}</div>

          </div>

          <div class="cart-icon vish">

            <div class="cart-buck"><a href="{{ wishlist }}"><img src="img/love.png"></a></div>

            <div class="cart-count">{{ wishlist_count }}</div>

          </div>

          {{ cart }}

          {# <div class="tp-mycart">

            <div class="my-c">My cart <br>

              $0.00 </div>

          </div> #}

        </div>

      </div>

    </div>

  </div>

  <!-- Uncomment below if you prefer to use an image logo -->

</header>





<div class="container">

  <div class="row">

    <div class="col-lg-12">

      <hr class="line-top ">

     
{{ menu }}

    </div>

  </div>

</div>





</div>

